    <splash density="port-xxxhdpi" src="www/res/screen/android/drawable-port-xxxhdpi-screen.png" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <name>laisa</name>
    <description>
        Shara a ride with friends
    </description>
    <author email="info@sqaaps.co.za" href="https://www.sqaaps.co.za">
        sqAAps
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="phonegap-version" value="cli-6.3.1" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:+27798665832" />
    <allow-intent href="sms:+27798665832" />
    <allow-intent href="mailto:thabang@sqaaps.co.za" />
